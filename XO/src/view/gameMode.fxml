<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="516.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="view.GameModeController">
   <children>
      <AnchorPane VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="threePlayersBtn" layoutX="259.0" layoutY="88.0" mnemonicParsing="false"
                    onAction="#onThreePlayersBtnClicked" text="3 Players" AnchorPane.bottomAnchor="10.0"
                    AnchorPane.leftAnchor="300.0" AnchorPane.rightAnchor="300.0" AnchorPane.topAnchor="10.0">
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
               </padding>
               <font>
                  <Font size="37.0"/>
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
         </VBox.margin>
      </AnchorPane>
      <AnchorPane VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
         </VBox.margin>
         <children>
            <Button fx:id="botBtn" layoutX="387.0" layoutY="67.0" mnemonicParsing="false" onAction="#onBotBtnClicked"
                    text="Game with bot" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="300.0"
                    AnchorPane.rightAnchor="300.0" AnchorPane.topAnchor="10.0">
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
               </padding>
               <font>
                  <Font size="37.0"/>
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
